<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
            <corners android:radius="28.0dip" />
            <size
                android:width="@dimen/settingslib_illustration_width"
                android:height="@dimen/settingslib_illustration_height" />
            <padding
                android:left="16dp"
                android:right="16dp"
                android:bottom="8dp"
                android:top="8dp"/>
            <stroke
                android:width="1dp"
                android:color="@android:color/system_accent1_100"/>
        </shape>
    </item>
</layer-list>
